.header-wrapper{
  background:#7287ae; 
  border: 0;
}
.btn_new{
  background: #7287ae;
  border: 0;
  box-shadow: none;
  max-width: 335px;
  width: 100%;
  padding: 21px 32px;
  border-radius: 40px;
  font-size: 24px;
}
.btn_new:hover{
 box-shadow:none !important; 
}
.header__icons{
  display:none; 
}
.header__inline-menu{
 text-align:right; 
}
.header__menu-item,.header__active-menu-item{
  color:#fff; 
  font-family:Ubuntu, sans-serif !important;
  font-size:25px;
}
.header__menu-item:hover{
  color:#fff;
}
.header__menu-item span{
  text-decoration:none; 
}
.image_with_text_new .image-with-text{
  margin:0;
}
.image_with_text_new .image-with-text__heading{
 font-weight:600; 
  font-size:58px;
  max-width:500px;
}
.image_with_text_new .image-with-text__text.rte {
    font-size: 29px;
    line-height: 32px;
    max-width: 570px;
}

.rich_text_new .rich-text{
  background: #7287ae;
  margin: 0 !important;
  padding: 100px 0 75px 0; 
}
.rich_text_new h2{
  color: #fff;
  font-size: 102px;
  line-height: 114px;
  font-weight: 600;
}
.rich_text_new .rich-text__text{
  font-size: 33px;
  color: #fff;
  max-width: 628px;
  margin: 0 auto;
  margin-top: 20px;
  line-height: 42px;
}
.multi_column_new .multicolumn
{
  background: #7287ae;
  margin: 0 !important; 
  padding: 82px 0;

}
.multi_column_new .multicolumn-card{
  background:transparent!important; 
}
.multi_column_new .title-wrapper-with-link{
  margin-top:0 !important;
}
.multi_column_new .title{
  font-size:48px;
  text-align:center;
  color:#fff;
  width:100%;
  text-transform:uppercase;
}
.multi_column_new h3{
  font-size:46px !important;
  color:#fff !important;
}
.multi_column_new .rte{
  font-size:20px;
  line-height:25px;
  color:#fff;
}
.multi_column_new .multicolumn-card__image-wrapper--third-width {
    width: 20%;
}
.footer{
 display:none; 
}
@media(max-width:768px){
  .rich_text_new h2 {
    color: #fff;
    font-size: 64px;
    line-height: 88px; 
  }
  .rich_text_new .rich-text__text {
    font-size: 30px;
    line-height: 42px;
  }
 .image_with_text_new .image-with-text .grid{
    flex-direction:column; 
  }
  .image_with_text_new .grid__item{
   width:100%;
  }
  .multi_column_new .multicolumn-list{
    flex-direction:column; 
  }
  .multi_column_new .multicolumn-list__item{
   width:100%; 
  }
  .image_with_text_new .image-with-text__heading{
   font-size:42px; 
  }
  .image_with_text_new .image-with-text__text.rte {
    font-size: 24px;
    line-height: 32px;

  }
}